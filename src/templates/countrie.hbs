{{#each this}}
      <article>
        <img src="{{flags.svg}}" alt="{{name}}" width="480">
        <h2>{{name.official}}</h2>
        <p>Capital: {{capital}}</p>
        <p>Population: {{population}}</p>
        <p>Languages: {{#each languages}}<span> {{this.name}}</span>{{/each}}</p>
      </article>
{{/each}}